<ul>
    <% if current_user %>
        <li>
        <%= link_to t('button.home'), show_user_path %>
        </li>
        <li>
        <%= link_to t('button.link'), links_path %>
        </li>
        <li>
        <%= link_to t('button.logout'), logout_path %>
        </li>
    <% else %>
        <li>
        <%= link_to t('button.login'), login_path %>
        </li>
        <li>
        <%= link_to t('button.register'), register_path %>
        </li>
    <% end %>
</ul>